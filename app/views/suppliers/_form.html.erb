<%= form_with(model: @supplier) do |form| %>
    <div>
      <%= form.label :trade_name %>
      <%= form.text_field :trade_name %>
    </div>
    <div>
      <%= form.label :corporate_name %>
      <%= form.text_area :corporate_name %>
    </div>
    <div>
      <%= form.label :nif %>
      <%= form.text_field :nif %>
    </div>
    <div>
      <%= form.label :address %>
      <%= form.text_field :address %>
    </div>
    <div>
      <%= form.label :email %>
      <%= form.text_field :email %>
    </div>
    <div>
      <%= form.label :phone_number %>
      <%= form.text_field :phone_number %>
    </div>
    <div>
      <%= form.submit 'Enviar' %>
    </div>
<% end %>